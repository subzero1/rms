<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.netsky.base.dataObjects.Ta25_face_module"  table="Ta25_face_module">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta25_num</param>
            </generator>
        </id>

   <property
  name="module_name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="module_name"
  />

  <property
  name="role_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="role_id"
  />

  <property
  name="module_url"
  type="java.lang.String"
  update="true"
  insert="true"
  column="module_url"
  />

  <property
  name="ui_type"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="ui_type"
  />


    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="Ta02_station"  table="$table_name">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta02_num</param>
            </generator>
        </id>

   <property
  name="flag"
  type="java.lang.String"
  update="true"
  insert="true"
  column="flag"
  />

  <property
  name="name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="name"
  />

  <property
  name="remark"
  type="java.lang.String"
  update="true"
  insert="true"
  column="remark"
  />

  <property
  name="seq"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="seq"
  />


    </class>

</hibernate-mapping>

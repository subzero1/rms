<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.netsky.base.dataObjects.Tz01_exception"  table="Tz01_exception">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tz01_num</param>
            </generator>
        </id>

   <property
  name="optime"
  type="java.util.Date"
  update="true"
  insert="true"
  column="optime"
  />

  <property
  name="opclass"
  type="java.lang.String"
  update="true"
  insert="true"
  column="opclass"
  />

  <property
  name="name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="name"
  />

  <property
  name="stacktrace"
  type="java.lang.String"
  update="true"
  insert="true"
  column="stacktrace"
  />


    </class>

</hibernate-mapping>

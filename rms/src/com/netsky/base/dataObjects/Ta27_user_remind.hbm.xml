<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="Ta27_user_remind"  table="$table_name">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta27_num</param>
            </generator>
        </id>

   <property
  name="user_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="user_id"
  />

  <property
  name="remind_type"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="remind_type"
  />

  <property
  name="mobile_flag"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="mobile_flag"
  />

  <property
  name="message_flag"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="message_flag"
  />


    </class>

</hibernate-mapping>

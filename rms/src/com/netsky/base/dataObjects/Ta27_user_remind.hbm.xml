<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Ta27_user_remind"
        table="Ta27_user_remind"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Ta27_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Ta27_user_remind.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="user_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="remind_type"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="remind_type"
        />

        <property
            name="mobile_flag"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="mobile_flag"
        />

        <property
            name="message_flag"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="message_flag"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Ta27_user_remind.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

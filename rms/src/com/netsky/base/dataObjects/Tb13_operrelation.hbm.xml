        />

        <property
            name="dest_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="dest_id"
        />

        <property
            name="tb12_ord"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="tb12_ord"
        />

        <property
            name="relation_name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="relation_name"
        />

        <property
            name="oper_time"
            type="java.util.Date"
            update="true"
            insert="true"
            column="oper_time"
        />

        <property
            name="seq"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="seq"
        />

        <!--
            To add non XDoclet property mappings, create a file named

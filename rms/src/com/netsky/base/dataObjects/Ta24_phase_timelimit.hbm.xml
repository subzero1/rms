<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.netsky.base.dataObjects.Ta24_phase_timelimit"  table="Ta24_phase_timelimit">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta24_num</param>
            </generator>
        </id>

   <property
  name="phase_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="phase_id"
  />

  <property
  name="phase_name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="phase_name"
  />

  <property
  name="gclb"
  type="java.lang.String"
  update="true"
  insert="true"
  column="gclb"
  />

  <property
  name="timelimit"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="timelimit"
  />


    </class>

</hibernate-mapping>

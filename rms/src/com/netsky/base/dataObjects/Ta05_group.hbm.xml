<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Ta05_group"
        table="Ta05_group"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Ta05_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Ta05_group.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="grade"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="grade"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="up_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="up_id"
        />

        <property
            name="dis"
            type="java.lang.String"
            update="true"
            insert="true"
            column="dis"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Ta05_group.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

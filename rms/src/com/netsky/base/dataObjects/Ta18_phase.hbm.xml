<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Ta18_phase"
        table="Ta18_phase"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Ta18_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Ta18_phase.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="seq"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="seq"
        />

        <property
            name="module_ids"
            type="java.lang.String"
            update="true"
            insert="true"
            column="module_ids"
        />

        <property
            name="node_names"
            type="java.lang.String"
            update="true"
            insert="true"
            column="node_names"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Ta18_phase.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

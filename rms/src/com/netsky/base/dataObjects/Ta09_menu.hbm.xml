<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Ta09_menu"
        table="Ta09_menu"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">S07_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Ta09_menu.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="url"
            type="java.lang.String"
            update="true"
            insert="true"
            column="url"
        />

        <property
            name="target"
            type="java.lang.String"
            update="true"
            insert="true"
            column="target"
        />

        <property
            name="rel"
            type="java.lang.String"
            update="true"
            insert="true"
            column="rel"
        />

        <property
            name="up_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="up_id"
        />

        <property
            name="seq"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="seq"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Ta09_menu.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

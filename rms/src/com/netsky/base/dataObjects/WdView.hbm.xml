        />

        <property
            name="ml_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="ml_id"
        />

        <property
            name="mlmc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="mlmc"
        />

        <property
            name="contents"
            type="java.lang.String"

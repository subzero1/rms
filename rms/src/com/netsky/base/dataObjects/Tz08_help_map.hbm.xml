<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.netsky.base.dataObjects.Tz08_help_map"  table="Tz08_help_map">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tz08_num</param>
            </generator>
        </id>

	  <property
	  name="module_name"
	  type="java.lang.String"
	  update="true"
	  insert="true"
	  column="module_name"
	  />
	
	  <property
	  name="help_id"
	  type="java.lang.Long"
	  update="true"
	  insert="true"
	  column="help_id"
	  />

    </class>

</hibernate-mapping>

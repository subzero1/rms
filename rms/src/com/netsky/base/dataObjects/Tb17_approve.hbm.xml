<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tb17_approve"
        table="Tb17_approve"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Tb17_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tb17_approve.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="project_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="project_id"
        />

        <property
            name="module_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="module_id"
        />

        <property
            name="user_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="user_name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="user_name"
        />

        <property
            name="station"
            type="java.lang.String"
            update="true"
            insert="true"
            column="station"
        />

        <property
            name="oper_time"
            type="java.util.Date"
            update="true"
            insert="true"
            column="oper_time"
        />

        <property
            name="check_result"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="check_result"
        />

        <property
            name="check_idea"
            type="java.lang.String"
            update="true"
            insert="true"
            column="check_idea"
        />

        <property
            name="node_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="node_id"
        />

        <property
            name="doc_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="doc_id"
        />

        <property
            name="opernode_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="opernode_id"
        />

        <property
            name="result_str"
            type="java.lang.String"
            update="true"
            insert="true"
            column="result_str"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tb17_approve.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tz02_oper_log"
        table="Tz02_oper_log"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Tz02_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tz02_oper_log.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="optime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="optime"
        />

        <property
            name="opuser"
            type="java.lang.String"
            update="true"
            insert="true"
            column="opuser"
        />

        <property
            name="optype"
            type="java.lang.String"
            update="true"
            insert="true"
            column="optype"
        />

        <property
            name="optable"
            type="java.lang.String"
            update="true"
            insert="true"
            column="optable"
        />

        <property
            name="old_data"
            type="java.lang.String"
            update="true"
            insert="true"
            column="old_data"
        />

        <property
            name="new_data"
            type="java.lang.String"
            update="true"
            insert="true"
            column="new_data"
        />

        <property
            name="login_id"
            type="java.lang.String"
            update="true"
            insert="true"
            column="login_id"
        />

        <property
            name="opdesc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="opdesc"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tz02_oper_log.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="Ta03_user"  table="$table_name">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta03_num</param>
            </generator>
        </id>

   <property
  name="name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="name"
  />

  <property
  name="login_id"
  type="java.lang.String"
  update="true"
  insert="true"
  column="login_id"
  />

  <property
  name="passwd"
  type="java.lang.String"
  update="true"
  insert="true"
  column="passwd"
  />

  <property
  name="fix_tel"
  type="java.lang.String"
  update="true"
  insert="true"
  column="fix_tel"
  />

  <property
  name="mobile_tel"
  type="java.lang.String"
  update="true"
  insert="true"
  column="mobile_tel"
  />

  <property
  name="email"
  type="java.lang.String"
  update="true"
  insert="true"
  column="email"
  />

  <property
  name="remark"
  type="java.lang.String"
  update="true"
  insert="true"
  column="remark"
  />

  <property
  name="dept_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="dept_id"
  />

  <property
  name="useflag"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="useflag"
  />

  <property
  name="last_pwd_date"
  type="java.util.Date"
  update="true"
  insert="true"
  column="last_pwd_date"
  />

  <property
  name="sex"
  type="java.lang.String"
  update="true"
  insert="true"
  column="sex"
  />

  <property
  name="search_level"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="search_level"
  />

  <property
  name="area_name"
  type="java.lang.String"
  update="true"
  insert="true"
  column="area_name"
  />

  <property
  name="new_flag"
  type="java.lang.String"
  update="true"
  insert="true"
  column="new_flag"
  />


    </class>

</hibernate-mapping>

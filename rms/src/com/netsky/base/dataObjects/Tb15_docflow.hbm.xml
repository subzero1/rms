        />

        <property
            name="accept_time"
            type="java.util.Date"
            update="true"
            insert="true"
            column="accept_time"
        />

        <property
            name="write_limit"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="write_limit"
        />

        <property
            name="huser_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="huser_id"
        />

        <property
            name="HWrite_limit"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="hWrite_limit"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tb15_docflow.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tb15_docflow"
        table="Tb15_docflow"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Tb15_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tb15_docflow.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="project_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="project_id"
        />

        <property
            name="node_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="node_id"
        />

        <property
            name="doc_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="doc_id"
        />

        <property
            name="user_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="doc_status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="doc_status"
        />

        <property
            name="module_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="module_id"
        />

        <property
            name="oper_status"
            type="java.lang.String"
            update="true"
            insert="true"
            column="oper_status"
        />

        <property
            name="oper_user"
            type="java.lang.String"
            update="true"
            insert="true"
            column="oper_user"
        />

        <property
            name="oper_time"
            type="java.util.Date"
            update="true"
            insert="true"
            column="oper_time"
        />

        <property
            name="group_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="group_id"
        />

        <property
            name="approve_result"
            type="java.lang.Integer"
            update="true"
            insert="true"

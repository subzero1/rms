<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tb01_flow"
        table="Tb01_flow"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Tb01_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tb01_flow.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="startNode"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="startNode"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="display"
            type="java.lang.String"
            update="true"
            insert="true"
            column="display"
        />

        <property
            name="type"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="type"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tb01_flow.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tz07_dataobject_cfg"
        table="Tz07_dataobject_cfg"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">TZ07_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tz07_dataobject_cfg.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="obj_desc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="obj_desc"
        />

        <property
            name="status"
            type="java.lang.String"
            update="true"
            insert="true"
            column="status"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tz07_dataobject_cfg.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

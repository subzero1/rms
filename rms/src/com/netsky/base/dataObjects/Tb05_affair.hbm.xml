<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.netsky.base.dataObjects.Tb05_affair"
        table="tb05_affair"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">Tb05_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tb05_affair.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="affair_type"
            type="java.lang.String"
            update="true"
            insert="true"
            column="affair_type"
        />

        <property
            name="doclass"
            type="java.lang.String"
            update="true"
            insert="true"
            column="doclass"
        />

        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            column="description"
        />

        <property
            name="paras"
            type="java.lang.String"
            update="true"
            insert="true"
            column="paras"
        />

        <property
            name="excute_sql"
            type="java.lang.String"
            update="true"
            insert="true"
            column="excute_sql"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tb05_affair.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

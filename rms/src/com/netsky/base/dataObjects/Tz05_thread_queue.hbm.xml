<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="Tz05_thread_queue"  table="$table_name">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tz05_num</param>
            </generator>
        </id>

   <property
  name="servicename"
  type="java.lang.String"
  update="true"
  insert="true"
  column="servicename"
  />

  <property
  name="parameters"
  type="java.lang.String"
  update="true"
  insert="true"
  column="parameters"
  />

  <property
  name="inserttime"
  type="java.util.Date"
  update="true"
  insert="true"
  column="inserttime"
  />

  <property
  name="handletime"
  type="java.util.Date"
  update="true"
  insert="true"
  column="handletime"
  />

  <property
  name="batchno"
  type="java.lang.String"
  update="true"
  insert="true"
  column="batchno"
  />

  <property
  name="status"
  type="java.lang.String"
  update="true"
  insert="true"
  column="status"
  />

  <property
  name="type"
  type="java.lang.String"
  update="true"
  insert="true"
  column="type"
  />

  <property
  name="remark"
  type="java.lang.String"
  update="true"
  insert="true"
  column="remark"
  />


    </class>

</hibernate-mapping>

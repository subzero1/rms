<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.rms.dataObjects.wxdw.Tf14_jlrj"
        table="Tf14_jlrj"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tf14_num</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tf14_jlrj.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="project_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="project_id"
        />

        <property
            name="user_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="create_date"
            type="java.util.Date"
            update="true"
            insert="true"
            column="create_date"
        />

        <property
            name="gczt"
            type="java.lang.String"
            update="true"
            insert="true"
            column="gczt"
        />

        <property
            name="xcms"
            type="java.lang.String"
            update="true"
            insert="true"
            column="xcms"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tf14_jlrj.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

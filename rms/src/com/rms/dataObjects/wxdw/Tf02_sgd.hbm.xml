        </id>

        <property
            name="mc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="mc"
        />

        <property
            name="sgdw_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="sgdw_id"
        />

        <property
            name="bz"
            type="java.lang.String"
            update="true"
            insert="true"

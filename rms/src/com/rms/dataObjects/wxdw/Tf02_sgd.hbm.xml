        table="tf02_sgd"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">TF02_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.rms.dataObjects.wxdw.Tf04_wxdw_user"  table="Tf04_wxdw_user">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tf04_num</param>
            </generator>
        </id>

   <property
  name="wxdw_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="wxdw_id"
  />

  <property
  name="user_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="user_id"
  />

  <property
  name="area"
  type="java.lang.String"
  update="true"
  insert="true"
  column="area"
  />
  
  <property
  name="zy"
  type="java.lang.String"
  update="true"
  insert="true"
  column="zy"
  />


    </class>

</hibernate-mapping>

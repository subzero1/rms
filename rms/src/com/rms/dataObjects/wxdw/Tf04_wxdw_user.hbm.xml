<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.rms.dataObjects.wxdw.Tf04_wxdw_user"
        table="tf04_wxdw_user"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">TF04_NUM</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Tf04_wxdw_user.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="user_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="wxdw_id"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="wxdw_id"
        />

        <property
            name="area"
            type="java.lang.String"
            update="true"
            insert="true"
            column="area"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Tf04_wxdw_user.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

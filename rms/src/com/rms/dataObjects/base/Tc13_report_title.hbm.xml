<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.rms.dataObjects.base.Tc13_report_title"  table="Tc13_report_title">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">tc13_num</param>
            </generator>
        </id>

   <property
  name="title"
  type="java.lang.String"
  update="true"
  insert="true"
  column="title"
  />

  <property
  name="remark"
  type="java.lang.String"
  update="true"
  insert="true"
  column="remark"
  />

  <property
  name="uri"
  type="java.lang.String"
  update="true"
  insert="true"
  column="uri"
  />

  <property
  name="ext"
  type="java.lang.String"
  update="true"
  insert="true"
  column="ext"
  />


    </class>

</hibernate-mapping>

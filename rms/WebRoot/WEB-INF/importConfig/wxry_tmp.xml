		<columns>
			<!-- byIndex:按列所在位置获取,byName: 按标题行名称获取-->
			<type>byName</type>
			<!-- 标题行位置 -->
			<titleRow>0</titleRow>
			<column>
					<columnName>NAME</columnName>
					<index/>
					<name>姓名</name>
					<colName>姓名</colName>
			</column>
			<column>
					<columnName>SEX</columnName>

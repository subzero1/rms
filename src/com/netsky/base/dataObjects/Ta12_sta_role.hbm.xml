<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="Ta12_sta_role"  table="$table_name">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta12_num</param>
            </generator>
        </id>

   <property
  name="station_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="station_id"
  />

  <property
  name="role_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="role_id"
  />


    </class>

</hibernate-mapping>

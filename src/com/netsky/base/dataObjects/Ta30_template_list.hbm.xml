<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="com.netsky.base.dataObjects.Ta30_template_list"  table="Ta30_template_list">

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="sequence">
                <param name="sequence">ta30_num</param>
            </generator>
        </id>

   <property
  name="parent_id"
  type="java.lang.Long"
  update="true"
  insert="true"
  column="parent_id"
  />

  <property
  name="type"
  type="java.lang.String"
  update="true"
  insert="true"
  column="type"
  />

  <property
  name="key"
  type="java.lang.String"
  update="true"
  insert="true"
  column="key"
  />

  <property
  name="value"
  type="java.lang.String"
  update="true"
  insert="true"
  column="value"
  />


    </class>

</hibernate-mapping>
